      "settings": {
        "title": "School Programmes ",
        "meta_descr": "Looking for learning activities that engage learners in a fun and positive way? Our school enrichment, curriculum programmes, and dynamic workshops are designed to maximise the potential for positive development.",
        "order": 13
      },
      "components": [
